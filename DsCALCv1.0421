    function tabE(obj, e) {
      var e = typeof event != "undefined" ? window.event : e // IE : Moz
      if (e.keyCode == 13) {
        //13=ENTER key
        var ele = document.forms[0].elements
        for (var i = 0; i < ele.length; i++) {
          var q = i == ele.length - 1 ? 0 : i + 1 // if last element : if any other
          if (obj == ele[i]) {
    //        console.log("q is  " + q)
            /*
                                console.log("ele.length   " + ele.length);
                                console.log("obj   " + obj.id);
                                console.log("ele   " + ele.id);
                                console.log("q   " + q);
                                console.log("ele[i]   " + ele[i].id);
                  */
            if (q == 1) {
              //skip buttons button_steendrukMIN+button_steendrukMAX
              q = 2 //go to Lengte1
              console.log("q next  " + q)
            } else if (q == 5) {
              //skip Lengte2
              q = 6 //go to Breedte1
              console.log("q next  " + q)
            } else if (q == 7) {
              q = 16
              console.log("q next  " + q)
            } else if (q == 17) {
              q = 17
              console.log("q next  " + q)
            } else if (q == 18) {
              q = 23
              console.log("q next  " + q)
            } else if (q == 24) {
              q = 29
              console.log("q next  " + q)
            } else if (q == 25) {
              q = 25
              console.log("q next  " + q)
            } else if (q > 30) {
              q = 3 // focus on button copy steendrukMAX, press ENTER=copied value
              console.log("ELSE q next " + q)
            }

            ele[q].focus()
            break
          }
        }
        return false
      }
    }
    
    
